{
  "presets": [
    ["@babel/preset-env", {
      "modules": "auto"
    }],
    "@babel/preset-typescript",
    ["@babel/preset-react", {
      "runtime": "automatic"
    }]
  ],
  "env": {
    "umd": {
      "presets": [
        ["@babel/preset-env", {
          "modules": "umd"
        }]
      ]
    },
    "cjs": {
      "presets": [
        ["@babel/preset-env", {
          "modules": "commonjs"
        }]
      ]
    },
    "esm": {
      "presets": [
        ["@babel/preset-env", {
          "modules": false
        }]
      ]
    }
  }
}
